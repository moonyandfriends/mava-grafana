# Grafana Configuration
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin123!@#
GRAFANA_LOG_LEVEL=info

# Supabase Configuration (Required for Railway deployment)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# Grafana Plugins (comma-separated list)
GRAFANA_PLUGINS=grafana-clock-panel,grafana-simple-json-datasource,grafana-worldmap-panel,grafana-piechart-panel,grafana-polystat-panel,vonage-status-panel

# Prometheus Configuration (if using with-prometheus profile)
PROMETHEUS_PORT=9090

# Database Configuration (for external databases)
# Uncomment and configure if using external database instead of SQLite
# GRAFANA_DATABASE_TYPE=postgres
# GRAFANA_DATABASE_HOST=localhost:5432
# GRAFANA_DATABASE_NAME=grafana
# GRAFANA_DATABASE_USER=grafana
# GRAFANA_DATABASE_PASSWORD=grafana_password

# SMTP Configuration (for alerting)
# GRAFANA_SMTP_ENABLED=true
# GRAFANA_SMTP_HOST=smtp.gmail.com:587
# GRAFANA_SMTP_USER=your-email@gmail.com
# GRAFANA_SMTP_PASSWORD=your-app-password
# GRAFANA_SMTP_FROM_ADDRESS=your-email@gmail.com
# GRAFANA_SMTP_FROM_NAME=Grafana

# Security Configuration
# GRAFANA_SECRET_KEY=your-secret-key-here
# GRAFANA_COOKIE_SECURE=false
# GRAFANA_COOKIE_SAMESITE=lax

# External URL Configuration
# GRAFANA_ROOT_URL=http://localhost:3000
# GRAFANA_SERVE_FROM_SUB_PATH=false

# Logging Configuration
# GRAFANA_LOG_MODE=console file
# GRAFANA_LOG_LEVEL=info

# Feature Toggles
# GRAFANA_FEATURE_TOGGLES=enable_feature1,enable_feature2 