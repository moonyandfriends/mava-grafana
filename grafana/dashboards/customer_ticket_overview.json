{
  "id": null,
  "title": "Customer & Ticket Overview",
  "tags": ["mava", "overview"],
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "refresh": "5m",
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Customers",
      "datasource": {
        "type": "postgres",
        "uid": "supabase"
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT COUNT(*) as value FROM customers;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "auto"
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Total Tickets",
      "datasource": {
        "type": "postgres",
        "uid": "supabase"
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT COUNT(*) as value FROM tickets;",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "auto"
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Open Tickets",
      "datasource": {
        "type": "postgres",
        "uid": "supabase"
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT COUNT(*) as value FROM tickets WHERE status = 'open';",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "auto"
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
    },
    {
      "id": 4,
      "type": "barchart",
      "title": "Tickets by Status",
      "datasource": {
        "type": "postgres",
        "uid": "supabase"
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT status, COUNT(*) as count FROM tickets GROUP BY status ORDER BY count DESC;",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Tickets Created Over Time",
      "datasource": {
        "type": "postgres",
        "uid": "supabase"
      },
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT created_at AS time, COUNT(*) as value FROM tickets GROUP BY DATE_TRUNC('day', created_at) ORDER BY time;",
          "refId": "E"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
    }
  ]
} 