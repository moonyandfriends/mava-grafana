        {
          "datasource": {
            "type": "postgres",
            "uid": "supabase-uid"
          },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  COALESCE(c.name, 'Unknown') as \"Customer Name\",\n  c.client as \"Client\",\n  COUNT(t.id) as \"Ticket Count\",\n  MAX(t.created_at) as \"Last Ticket\"\nFROM customers c\nLEFT JOIN tickets t ON c.id = t.customer_id\nWHERE t.created_at >= $__timeFrom() AND t.created_at <= $__timeTo()\nGROUP BY c.id, c.name, c.client\nORDER BY \"Ticket Count\" DESC\nLIMIT 10",
          "refId": "A"
        }
      ],
      "title": "Most Active Customers",
      "type": "table"
