    {
      "id": 7,
      "title": "Ambassador Activity Timeline",
      "type": "timeseries",
      "datasource": {"type": "postgres", "uid": "supabase"},
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 10},
      "targets": [
        {
          "format": "time_series",
          "rawSql": "WITH daily_activity AS (SELECT date_trunc('day', dm.created_at) AS time, 'Discord' AS platform, COUNT(*) AS posts FROM discord_messages dm JOIN community_ambassadors ca ON dm.author_id::bigint = ca.discord_id WHERE dm.created_at >= $__timeFrom() AND dm.created_at <= $__timeTo() AND dm.author_is_bot = false GROUP BY date_trunc('day', dm.created_at) UNION ALL SELECT date_trunc('day', tm.date) AS time, 'Telegram' AS platform, COUNT(*) AS posts FROM telegram_messages tm JOIN community_ambassadors ca ON tm.from_user_id = ca.telegram_id WHERE tm.date >= $__timeFrom() AND tm.date <= $__timeTo() GROUP BY date_trunc('day', tm.date) UNION ALL SELECT date_trunc('day', dm.created_at) AS time, 'X (Twitter)' AS platform, COUNT(DISTINCT dm.id) AS posts FROM discord_messages dm CROSS JOIN community_ambassadors ca WHERE dm.channel_id = '1248750328896295084' AND dm.created_at >= $__timeFrom() AND dm.created_at <= $__timeTo() AND dm.author_is_bot = false AND dm.content ILIKE '%' || ca.x_handle || '%' GROUP BY date_trunc('day', dm.created_at)) SELECT time, platform AS metric, posts AS value FROM daily_activity ORDER BY time, platform;",
          "refId": "A"
        }
      ],
