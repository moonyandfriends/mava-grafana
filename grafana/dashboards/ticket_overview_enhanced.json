    {
      "id": 11,
      "title": "Priority Open Tickets",
      "type": "table",
      "datasource": {"type": "postgres", "uid": "supabase"},
      "gridPos": {"h": 12, "w": 24, "x": 0, "y": 21},
      "targets": [
        {
          "format": "table",
          "rawSql": "WITH latest_messages AS (SELECT m.ticket_id, m.content, m.created_at as message_created_at, m.sender, m.from_customer, ROW_NUMBER() OVER (PARTITION BY m.ticket_id ORDER BY m.created_at DESC) as rn FROM mava_messages m WHERE m.is_deleted = false AND m.content IS NOT NULL AND m.content != ''), ticket_priority AS (SELECT id, CASE WHEN status = 'Open' AND created_at < NOW() - INTERVAL '24 hours' THEN 'High' WHEN status = 'Pending' AND created_at < NOW() - INTERVAL '12 hours' THEN 'Medium' ELSE 'Low' END AS priority FROM mava_tickets_with_customers) SELECT t.id AS \"Ticket ID\", tp.priority AS \"Priority\", t.category AS \"Category\", COALESCE(t.customer_name, 'Unknown') AS \"Customer\", t.source_type AS \"Source\", t.assigned_to AS \"Assigned To\", t.created_at AS \"Created\", EXTRACT(epoch FROM (NOW() - t.created_at))/3600 AS \"Age (Hours)\", CASE WHEN lm.content IS NOT NULL THEN CASE WHEN LENGTH(lm.content) > 100 THEN SUBSTRING(lm.content, 1, 100) || '...' ELSE lm.content END ELSE 'No messages' END AS \"Latest Message\" FROM mava_tickets_with_customers t LEFT JOIN latest_messages lm ON t.id = lm.ticket_id AND lm.rn = 1 LEFT JOIN ticket_priority tp ON t.id = tp.id WHERE t.status IN ('Open', 'Pending', 'Waiting') ORDER BY CASE WHEN tp.priority = 'High' THEN 1 WHEN tp.priority = 'Medium' THEN 2 ELSE 3 END, t.created_at DESC LIMIT 50;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "filterable": true,
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
