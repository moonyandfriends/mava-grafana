    {
      "id": 1,
      "title": "Total Ambassadors",
      "type": "stat",
      "datasource": {"type": "postgres", "uid": "supabase"},
      "gridPos": {"h": 5, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "format": "table",
          "rawSql": "WITH active_ambassadors AS (\n  SELECT \n    COALESCE(dm.author_display_name, dm.author_username) as ambassador_name,\n    COUNT(dm.id) as message_count\n  FROM discord_messages dm\n  WHERE dm.created_at >= NOW() - INTERVAL '30 days'\n    AND dm.author_is_bot = false\n    AND dm.content IS NOT NULL\n    AND COALESCE(dm.author_display_name, dm.author_username) IS NOT NULL\n  GROUP BY COALESCE(dm.author_display_name, dm.author_username)\n  HAVING COUNT(dm.id) >= 10\n  UNION\n  SELECT \n    COALESCE(tm.from_user_first_name || ' ' || tm.from_user_last_name, tm.from_user_username) as ambassador_name,\n    COUNT(tm.id) as message_count\n  FROM telegram_messages tm\n  WHERE tm.date >= NOW() - INTERVAL '30 days'\n    AND tm.from_user_is_bot = false\n    AND tm.text IS NOT NULL\n    AND COALESCE(tm.from_user_first_name, tm.from_user_username) IS NOT NULL\n  GROUP BY COALESCE(tm.from_user_first_name || ' ' || tm.from_user_last_name, tm.from_user_username)\n  HAVING COUNT(tm.id) >= 5\n)\nSELECT COUNT(DISTINCT ambassador_name) AS total_ambassadors FROM active_ambassadors;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "rgba(50, 172, 45, 0.97)", "value": null},
              {"color": "rgba(237, 129, 40, 0.97)", "value": 10},
              {"color": "rgba(245, 54, 54, 0.97)", "value": 20}
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "background_solid",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "value_and_name",
        "wideLayout": true
      },
      "pluginVersion": "9.0.0"
    },
