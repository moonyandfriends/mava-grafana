    {
      "id": 4,
      "title": "First Response Rate",
      "type": "stat",
      "datasource": {"type": "postgres", "uid": "supabase"},
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 1},
      "targets": [
        {
          "format": "table",
          "rawSql": "WITH first_responses AS (SELECT t.id, CASE WHEN EXISTS (SELECT 1 FROM mava_messages m WHERE m.ticket_id = t.id AND m.from_customer = false AND m.created_at <= t.created_at + INTERVAL '4 hours') THEN 1 ELSE 0 END as responded_quickly FROM mava_tickets_with_customers t WHERE t.created_at >= $__timeFrom() AND t.created_at <= $__timeTo()) SELECT ROUND(AVG(responded_quickly) * 100, 1) as first_response_rate FROM first_responses;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
