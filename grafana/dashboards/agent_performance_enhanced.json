    {
      "id": 9,
      "title": "Agent Workload Distribution",
      "type": "table",
      "datasource": {"type": "postgres", "uid": "supabase"},
      "gridPos": {"h": 12, "w": 24, "x": 0, "y": 21},
      "targets": [
        {
          "format": "table",
          "rawSql": "WITH agent_metrics AS (SELECT COALESCE(assigned_to, 'Unassigned') as agent, COUNT(*) as total_tickets, COUNT(*) FILTER (WHERE status = 'Open') as open_tickets, COUNT(*) FILTER (WHERE status = 'Pending') as pending_tickets, COUNT(*) FILTER (WHERE status = 'Resolved') as resolved_tickets, COUNT(*) FILTER (WHERE status = 'Closed') as closed_tickets, AVG(EXTRACT(epoch FROM (updated_at - created_at))/3600) as avg_resolution_hours FROM mava_tickets_with_customers WHERE created_at >= $__timeFrom() AND created_at <= $__timeTo() GROUP BY assigned_to), agent_messages AS (SELECT COALESCE(t.assigned_to, 'Unassigned') as agent, COUNT(m.*) as message_count FROM mava_messages m JOIN mava_tickets_with_customers t ON m.ticket_id = t.id WHERE m.from_customer = false AND m.created_at >= $__timeFrom() AND m.created_at <= $__timeTo() GROUP BY t.assigned_to) SELECT am.agent AS \"Agent\", am.total_tickets AS \"Total Tickets\", am.open_tickets AS \"Open\", am.pending_tickets AS \"Pending\", am.resolved_tickets AS \"Resolved\", am.closed_tickets AS \"Closed\", ROUND(am.avg_resolution_hours, 1) AS \"Avg Resolution (Hours)\", COALESCE(amsg.message_count, 0) AS \"Messages Sent\", ROUND((am.resolved_tickets + am.closed_tickets) * 100.0 / NULLIF(am.total_tickets, 0), 1) AS \"Resolution Rate (%)\", CASE WHEN am.avg_resolution_hours <= 4 THEN 'Excellent' WHEN am.avg_resolution_hours <= 8 THEN 'Good' WHEN am.avg_resolution_hours <= 24 THEN 'Average' ELSE 'Needs Improvement' END AS \"Performance Rating\" FROM agent_metrics am LEFT JOIN agent_messages amsg ON am.agent = amsg.agent WHERE am.agent != 'Unassigned' ORDER BY am.total_tickets DESC;",
          "refId": "A"
        }
      ],
