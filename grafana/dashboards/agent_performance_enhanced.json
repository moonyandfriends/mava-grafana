    {
      "id": 2,
      "title": "Average Response Time",
      "type": "stat",
      "datasource": {"type": "postgres", "uid": "supabase"},
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1},
      "targets": [
        {
          "format": "table",
          "rawSql": "WITH response_times AS (SELECT m1.ticket_id, m1.created_at as customer_message_time, MIN(m2.created_at) as agent_response_time FROM mava_messages m1 JOIN mava_messages m2 ON m1.ticket_id = m2.ticket_id WHERE m1.from_customer = true AND m2.from_customer = false AND m2.created_at > m1.created_at AND m1.created_at >= $__timeFrom() AND m1.created_at <= $__timeTo() GROUP BY m1.ticket_id, m1.created_at) SELECT AVG(EXTRACT(epoch FROM (agent_response_time - customer_message_time))/3600) as avg_response_hours FROM response_times;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 2
              },
              {
                "color": "red",
                "value": 8
              }
            ]
          },
          "unit": "h"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
