          "rawSql": "WITH daily_metrics AS (SELECT date_trunc('day', created_at) as time, 'Tickets Created' as metric, COUNT(*) as value FROM mava_tickets_with_customers WHERE created_at >= $__timeFrom() AND created_at <= $__timeTo() GROUP BY date_trunc('day', created_at) UNION ALL SELECT date_trunc('day', updated_at) as time, 'Tickets Resolved' as metric, COUNT(*) as value FROM mava_tickets_with_customers WHERE status IN ('Resolved', 'Closed') AND updated_at >= $__timeFrom() AND updated_at <= $__timeTo() GROUP BY date_trunc('day', updated_at) UNION ALL SELECT date_trunc('day', created_at) as time, 'Agent Messages' as metric, COUNT(*) as value FROM mava_messages WHERE from_customer = false AND created_at >= $__timeFrom() AND created_at <= $__timeTo() GROUP BY date_trunc('day', created_at)) SELECT time, metric, value FROM daily_metrics ORDER BY time;",
          "refId": "A"
        }
