{
  "name": "Community Monitoring Theme",
  "type": "dark",
  "colors": {
    "primary": "#667eea",
    "secondary": "#764ba2",
    "success": "#28a745",
    "warning": "#ffc107",
    "error": "#dc3545",
    "info": "#17a2b8",
    "background": "#1e1e1e",
    "surface": "#2d2d2d",
    "text": "#ffffff",
    "textSecondary": "#b0b0b0",
    "border": "#3d3d3d",
    "hover": "#4a4a4a"
  },
  "visualization": {
    "palette": [
      "#667eea",
      "#764ba2",
      "#f093fb",
      "#f5576c",
      "#4facfe",
      "#00f2fe",
      "#fa709a",
      "#fee140",
      "#a8edea",
      "#fed6e3"
    ],
    "graph": {
      "colorMode": "continuous",
      "fillOpacity": 0.25,
      "lineWidth": 3,
      "pointSize": 6
    }
  },
  "panel": {
    "background": "#2d2d2d",
    "border": "#3d3d3d",
    "borderRadius": 8,
    "padding": 16,
    "margin": 8
  },
  "stat": {
    "colorMode": "background",
    "graphMode": "area",
    "textMode": "auto",
    "wideLayout": true
  },
  "table": {
    "cellHeight": "sm",
    "headerBackground": "#3d3d3d",
    "rowBackground": "#2d2d2d",
    "alternateRowBackground": "#333333",
    "borderColor": "#3d3d3d"
  },
  "timeseries": {
    "lineInterpolation": "smooth",
    "fillOpacity": 20,
    "gradientMode": "opacity",
    "showPoints": "auto",
    "legendDisplayMode": "table",
    "legendPlacement": "bottom"
  }
}